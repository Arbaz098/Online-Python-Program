   "id": "d213fc95",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter The Num Here:  7\n",
      "The Num is Odd\n"
     ]
    }
   ],
   "source": [
    "num=int(input(\"Enter The Num Here:  \"))\n",
    "if num%2==0:\n",
    "    print(\"The Num is Even\")\n",
    "else:\n",
    "    print(\"The Num is Odd\")"
